<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.mum.cs.cs401.main.BookUIController">
   <children>
      <GridPane hgap="5.0" vgap="10.0" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Look up Book" />
            <Button mnemonicParsing="false" onAction="#addBook" text="Add" GridPane.rowIndex="3" />
            <Button mnemonicParsing="false" onAction="#lookUpBook" text="Look" GridPane.rowIndex="1" />
            <Label text="add bookcopy" GridPane.rowIndex="2" />
            <TextField fx:id="lookUpTextField" GridPane.columnIndex="1" />
            <Label fx:id="LookResultLabel" text="Look up Book" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="addBookTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="addBookResultLabel" text="Look up Book" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
         <padding>
            <Insets bottom="10.0" left="25.0" right="25.0" top="10.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
