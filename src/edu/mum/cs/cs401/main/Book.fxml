<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane layoutX="200.0" layoutY="200.0" maxWidth="800.0" prefHeight="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.mum.cs.cs401.main.BookUIController">
   <children>
      <GridPane hgap="5.0" layoutX="30.0" layoutY="30.0" prefHeight="207.0" prefWidth="600.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="117.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="430.0" minWidth="10.0" prefWidth="428.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" />
          <RowConstraints />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Look up Book" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" onAction="#addBook" text="Add" GridPane.rowIndex="4" />
            <Button mnemonicParsing="false" onAction="#lookUpBook" text="Look" GridPane.rowIndex="2" />
            <Label text="add bookcopy" GridPane.rowIndex="3" />
            <TextField fx:id="lookUpTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="LookResultLabel" text="Look up result" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="addBookTextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label fx:id="addBookResultLabel" text="add book result" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Welcome!" />
         </children>
         <padding>
            <Insets bottom="10.0" left="25.0" right="25.0" top="10.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
