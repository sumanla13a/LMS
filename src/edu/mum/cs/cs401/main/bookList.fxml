<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.mum.cs.cs401.main.BookListUIController">
   <children>
      <BorderPane layoutX="0" layoutY="0" prefHeight="376.0" prefWidth="524.0">
         <top>
            <Label text="Member List" BorderPane.alignment="CENTER_LEFT">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </top>
         <center>
            <TableView fx:id="tblMembers" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="isbnColumn" prefWidth="105.0" text="ISBN" />
                <TableColumn fx:id="titleColumn" prefWidth="105.0" text="Title" />
              </columns>
            </TableView>
         </center>
         <bottom>
            <Button mnemonicParsing="false" onAction="#onSubmit" text="Refresh" BorderPane.alignment="CENTER_LEFT">
               <BorderPane.margin>
                  <Insets top="10.0" />
               </BorderPane.margin></Button>
         </bottom>
         <padding>
            <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
